        "from matplotlib import pyplot as plt\n",
        "\n",
        "labels = ['Geeks 1', 'Geeks 2', 'Geeks 3']\n",
        "sizes = [35, 35, 30]\n",
        "\n",
        "plt.pie(sizes, labels=labels, autopct='%1.1f%%', startangle=90)\n",
        "plt.title('Pie Chart Example')\n",
        "plt.show()"
      ]
